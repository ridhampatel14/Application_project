<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.application_project">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Application_project">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.application_project.RegisterActivity"
            android:parentActivityName="com.example.application_project.MainActivity">
        </activity>
        <activity
            android:name="com.example.application_project.SigninActivity"
            android:parentActivityName="com.example.application_project.MainActivity">
        </activity>
        <activity
            android:name="com.example.application_project.DetailActivity"
            android:parentActivityName="com.example.application_project.movie_main">
        </activity>
        <activity android:name="com.example.application_project.movie_main">
        </activity>
    </application>

</manifest>